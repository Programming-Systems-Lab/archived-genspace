/*
    class to store the geneways control parameters
*/
public class GenewaysControlParams {
    private int minInstanceCount;
    private int minArticleCount;
    private String direction;
    private Date newerThan;
    private Date olderThan;
    private DAGTypeComponent actionTypeDAG;

    public GenewaysControlParams(int minInstanceCount, int minArticleCount, String direction, Date newerThan, Date olderThan, DAGTypeComponent actionTypeDAG) {
        this.minInstanceCount = minInstanceCount;
        this.minArticleCount = minArticleCount;
        this.direction = direction;
        this.newerThan = newerThan;
        this.olderThan = olderThan;
        this.actionTypeDAG = actionTypeDAG;
    }

    public int getMinInstanceCount() {
        return minInstanceCount;
    }

    public void setMinInstanceCount(int minInstanceCount) {
        this.minInstanceCount = minInstanceCount;
    }

    public int getMinArticleCount() {
        return minArticleCount;
    }

    public void setMinArticleCount(int minArticleCount) {
        this.minArticleCount = minArticleCount;
    }

    public String getDirection() {
        return direction;
    }

    public void setDirection(String direction) {
        this.direction = direction;
    }

    public Date getNewerThan() {
        return newerThan;
    }

    public void setNewerThan(Date newerThan) {
        this.newerThan = newerThan;
    }

    public Date getOlderThan() {
        return olderThan;
    }

    public void setOlderThan(Date olderThan) {
        this.olderThan = olderThan;
    }

    public DAGTypeComponent getActionTypeDAG() {
        return actionTypeDAG;
    }

    public void setActionTypeDAG(DAGTypeComponent actionTypeDAG) {
        this.actionTypeDAG = actionTypeDAG;
    }
}

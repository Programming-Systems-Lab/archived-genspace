<!-- 
	A bare-bones all.xml file that is useful when creating a new component.
	
	author: keshav 
	version: $Id: all-genspace.xml,v 1.2 2008-12-08 21:39:03 nankin Exp $
-->

<geaw-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ConfigurationFile Schema.xsd">

	<description>
		This is useful when writing new components as it speeds up the
		starting time of geworkbench. To use, rename as all.xml.
	</description>

	<gui-window name="Main GUI"
		class="org.geworkbench.engine.skin.Skin" />

	<plugin id="projectWindow" name="Project Panel"
		class="org.geworkbench.builtin.projects.ProjectPanel">
		<gui-area name="ProjectArea" />
	</plugin>

	<plugin id="expressionFileFilter" name="Exp. Format"
		class="org.geworkbench.components.parsers.ExpressionFileFormat" />

	<!-- New Gene Panel implementation -->
	<plugin id="genePanel" name="Markers"
		class="org.geworkbench.components.selectors.GenePanel"
		source="selectors">
		<gui-area name="SelectionArea" />
	</plugin>

	<plugin id="phenotypePanel2" name="Arrays/Phenotypes"
		class="org.geworkbench.components.selectors.PhenotypePanel"
		source="selectors">
		<gui-area name="SelectionArea" />
	</plugin>

	<!-- 
		<plugin id="microarrayPanel" name="Microarray Viewer" class="org.geworkbench.components.microarrays.MicroarrayPanel" source="microarrays">
		<gui-area name="VisualArea"/>
		<subscription type="org.geworkbench.events.ProjectEvent" enabled="Yes"/>
		</plugin>
	-->
	<plugin id="hierarchicalClusteringPanel" name="Dendrogram"
		class="org.geworkbench.components.hierarchicalclustering.HierClusterViewAppComponent"
		source="hierarchicalclustering">
		<gui-area name="VisualArea" />
	</plugin>

	<!-- Analyses must go here, before the analysis panel -->
	<plugin id="fastHierarchicalClusteringAnalysis"
		name="Fast Hierachical Clustering Analysis"
		class="org.geworkbench.components.clustering.FastHierClustAnalysis"
		source="clustering" />
	<plugin id="medusa" name="MEDUSA Analysis"
		class="org.geworkbench.components.medusa.MedusaAnalysis"
		source="medusa" />

	<plugin id="analysisPanel" name="Analysis"
		class="org.geworkbench.components.analysis.AnalysisPanel"
		source="analysis">
		<gui-area name="CommandArea" />
	</plugin>

	<!-- Views of corresponding analyses go here -->
	<plugin id="medusaViewer" name="MEDUSA Viewer"
		class="org.geworkbench.components.medusa.gui.MedusaVisualComponent"
		source="medusa">
		<gui-area name="VisualArea" />
	</plugin>






	
	
	<plugin id="genspaceLogPreferences" name="genSpace Log Preferences"
		class="org.geworkbench.components.genspace.GenSpaceLogPreferences"
		source="genspace">
		<gui-area name="VisualArea" />
	</plugin>


	
	
	<plugin id="RealTimeWorkFlowSuggestion" name="R.T.W.F.S."
		class="org.geworkbench.components.genspace.RealTimeWorkFlowSuggestion"
		source="genspace">
		<gui-area name="VisualArea" />
	</plugin>
	
	
	<plugin id="WorkflowVisualization" name="WorkflowVisualization"
		class="org.geworkbench.components.genspace.WorkflowVisualization"
		source="genspace">
		<gui-area name="VisualArea" />
	</plugin>

	<plugin id="genspaceLogger" name="Genspace"
		class="org.geworkbench.components.genspace.GenspaceLogger"
		source="genspace">
	</plugin>

</geaw-config>